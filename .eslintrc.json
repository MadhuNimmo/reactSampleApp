{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "@typescript-eslint/explicit-function-return-type": [
            "off"
        ],
        "fetch-no-catch": [
            "error"
        ],
        "fetch-no-progress": [
            "error"
        ],
        "dont-call-x": [
            "error"
        ]
    }
}